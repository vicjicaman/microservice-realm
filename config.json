{
  "dependencies": {
    "microservice-config": {
      "url": "github.com:vicjicaman/microservice-config.git",
      "version": "1.70.3-master"
    }
  },
  "config": [
    {
      "name": "HOST",
      "value": "${HOST@microservice-config}"
    },
    {
      "name": "NAMESPACE",
      "value": "${NAMESPACE@microservice-config}"
    },
    {
      "name": "CERTIFICATES_CLAIM_NAME",
      "value": "certificates-volume-claim"
    },
    {
      "name": "CERTIFICATES_VOLUME_NAME",
      "value": "certificates-volume"
    },
    {
      "name": "CERTIFICATES_ACCOUNT_NAME",
      "value": "certificates-manager"
    },
    {
      "name": "CERTIFICATES_ROLE_NAME",
      "value": "certificates-manager-role"
    },
    {
      "name": "CERTIFICATES_SECRET_NAME",
      "value": "certificates-secret"
    },
    {
      "name": "CERTBOT_SECRET_NAME",
      "value": "cerbot-secret"
    },
    {
      "name": "CERTIFICATES_CRONJOB_NAME",
      "value": "certificates-cronjob"
    }
  ]
}